public List<Profile> GetTutee()
    List<Profile>Tutee = new List<Profile>()
    try
       command.CommandText = StoredProcedures.GetTutee;
       connection.Open;
       SqlDataReader reader = command.ExecuteReader();
       if (reader.HasRows)

            while (reader.Read())
           {    
               Profile tutee = new Profile

                 Roles = reader[@p_roles].ToString()
                 Name = reader[@p_name].ToString()
                 Email = reader[@p_email].ToString()
                 Bio = reader[@p_bio].ToString()
                 Schedule = reader[@p_schedule].ToString()

            };
            Tutee.Add(tutee);
           }
          }
        } 
        catch
             throw;
        {
        finally
        {
            connection.Close();
        }
        return tutee;
    

